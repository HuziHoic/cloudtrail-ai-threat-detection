{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ReadCloudTrailLogs",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::cloudtrail-logs-bucket-name",
        "arn:aws:s3:::cloudtrail-logs-bucket-name/AWSLogs/*"
      ]
    }
  ]
}
